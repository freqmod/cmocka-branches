get_filename_component(CMOCKA_BRANCHES_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

if (EXISTS "${CMOCKA_BRANCHES_CMAKE_DIR}/CMakeCache.txt")
    # In build tree
    include(${CMOCKA_BRANCHES_CMAKE_DIR}/cmocka-build-tree-settings.cmake)
else()
    set(CMOCKA_INCLUDE_DIR @INCLUDE_INSTALL_DIR@)
endif()

set(CMOCKA_LIBRARY @LIB_INSTALL_DIR@/@CMOCKA_LIBRARY_NAME@)
set(CMOCKA_LIBRARIES @LIB_INSTALL_DIR@/@CMOCKA_LIBRARY_NAME@)
